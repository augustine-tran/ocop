<ul class="flex flex-col space-y-8 divide-dashed divide-y-2">
  <%= render partial: 'node_roots', collection: @final_assessment.scores.node_roots, as: :node %>
</ul>
<% content_for :page_header do %>
  <%= t('.show_differences', name: @final_assessment.name) %>
<% end %>
<% content_for :quick_action do %>
  <%= link_to final_submission_path(@final_assessment.submission), class: "relative inline-flex items-center gap-x-1.5 rounded-md bg-indigo-500 px-3 py-2 text-sm font-semibold text-white shadow-sm hover:bg-indigo-400 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-500" do %>
    <%= render 'shared/icons/add_button' %>
    <%= t('.back') %>
  <% end %>
<% end %>
